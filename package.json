{
  "name": "react-blockly-drawer",
  "version": "1.6.2",
  "description": "A React component to play with blockly.",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d build",
    "test": "jest --env=jsdom --no-watchman",
