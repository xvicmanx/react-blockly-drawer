    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "^21.2.1",
    "np": "^2.18.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "watch": "^1.0.2"
  }
